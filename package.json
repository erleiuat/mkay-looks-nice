        "@nuxtjs/eslint-config-typescript": "^6.0.1",
        "@supabase/supabase-js": "^1.29.4",
        "autoprefixer": "^10.3.7",
        "daisyui": "^1.16.0",
        "eslint": "^8.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.4",
        "lint-staged": "^11.2.3",
        "npm-run-all": "^4.1.5",
        "nuxt3": "^3.0.0-27389502.88da719",
        "postcss": "^8.3.11",
        "prettier": "^2.4.1",
        "tailwindcss": "^2.2.17",
        "typescript": "^4.4.4",
        "unplugin-icons": "^0.12.16"
    },
    "dependencies": {},
    "husky": {
